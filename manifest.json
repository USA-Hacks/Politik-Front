{
    "manifest_version": 2,
    "name": "Politik",
    "description": "Find what candidate you indentify with!",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "main.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ]
}
